{
  "name": "SOCKS5 Proxy (Dante)",
  "version": "1.0.1",
  "slug": "socks5",
  "description": "Dante SOCKS5 server with user authentication. Ensure the configured port is not in use.",
  "arch": ["aarch64", "armv7", "amd64"],
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "options": {
    "port": 1080,
    "interface": "eth0",
    "users": [
      {
        "username": "proxyuser",
        "password": "proxypass"
      }
    ]
  },
  "schema": {
    "port": "int",
    "interface": "str",
    "users": [
      {
        "username": "str",
        "password": "str"
      }
    ]
  },
  "build": true
}
